@page
@model Exwhyzee.AhiomaDashboard.MainWebsite.Pages.SuccessModel
@{
    ViewData["Title"] = "Success";
    Layout = "/Pages/Shared/_WebLayout.cshtml";
}

<partial name="_header_One" />

<div class="main-body clearfix sidebar--left">
    <div class="container">
        <div class="sb-breadcrumbs breadcrumb-bg ">
            <ul class="breadcrumb ">
                <li class="breadcrumb-item ">
                    <i class="fa fa-home"></i>
                    <a href="/" class="breadcrumb-label">Home</a>
                </li>

                <li class="breadcrumb-item is-active">
                    <a href="#" class="breadcrumb-label">Shop Registeration</a>
                </li>

            </ul>

        </div>


        <div class="account account--fixed">
            <div class="account-body">
                <div class="row">
                    <div class="col-md-3"></div>
                    <div class="col-md-6">
                        <div class="alert alert-warning">
                            <h3>@TempData["success"]</h3>
                            <h5>
                                To know more visit <a asp-page="./FAQs">FAQ</a>
                            </h5>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

