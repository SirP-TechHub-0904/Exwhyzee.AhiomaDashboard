@model ICollection<Exwhyzee.AhiomaDashboard.EntityFramework.Tables.ProductUploadShop>

@if (Model.Count > 0)
{
    <li class="menu-title">Third Party Shops</li>
    @foreach (var item in Model)
    {
        <li>
            <a asp-area="Admin" asp-page="/Products/Index" asp-route-id="@item.TenantId" asp-route-uid="@TempData["uid"]" class="waves-effect">
                <i class="fa fa-building"></i>
                <span>@item.Tenant.BusinessName Shop</span>
            </a>
        </li>

    }
}