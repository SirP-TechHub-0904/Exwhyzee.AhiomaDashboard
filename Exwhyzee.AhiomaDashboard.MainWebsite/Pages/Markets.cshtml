<style>
    
    .page-content-wrapper {
        position: relative;
        z-index: 1;
        margin-top: 20px;
        margin-bottom: 58px;
    }
</style>
<div class="page-content-wrapper">
    <!-- Top Products-->
    <div class="top-products-area py-3">
        <div class="container">
            <div class="section-heading d-flex align-items-center justify-content-between">
                <h6>Markets</h6>
                <!-- Layout Options-->
            </div>

        </div>
        <div class="container">
           
            <div class="row g-3">


                @foreach (var data in Model.Markets)
                {
<div class="col-6 col-sm-4">
    <div class="card blog-catagory-card" style="min-height:50px;padding:8px;">
        <h4 class="card-title" style="padding-bottom:0px;margin-bottom:3px;font-size:12px;color:#ff6a00;font-weight:800;">
            <a style="color: #ff6a00;" asp-page="./MarketDetails" asp-route-customerRef="" asp-route-id="@data.Id" asp-route-name="@data.Name.Replace(" ", "-")" asp-route-mktstate="@data.State.Replace(" ", "-")" asp-route-mktaddress="@data.Address.Replace(" ", "-")" tabindex="0" title="@data.Name">
                @data.Name
            </a>
            </h4>

            <h6 class="card-title" style="padding-bottom:0px;margin-bottom:3px;font-size:10px;">
                <a asp-page="./StateMarket" asp-route-customerRef="" asp-route-state="@data.State" asp-route-Market="@data.Name" tabindex="0" title="@data.Name">@data.State STATE</a>
            </h6>
    </div>
</div>}







            </div>
        </div>
    </div>
</div>


