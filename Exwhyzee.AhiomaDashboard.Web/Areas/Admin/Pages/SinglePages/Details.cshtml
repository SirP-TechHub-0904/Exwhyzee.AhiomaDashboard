@page
@model Exwhyzee.AhiomaDashboard.Web.Areas.Admin.Pages.SinglePages.DetailsModel

@{
    ViewData["Title"] = "Details";
}
<div class="row">
    <div class="card-columns mx-auto d-flex justify-content-center col-12">

        <div class="card">
            <div class="card-header">

                <h3 class="card-title">
                    Detail Page


                </h3>
            </div>
            <div class="card-body">
                <dl class="dl-horizontal">
                    <dt>
                        @Html.DisplayNameFor(model => model.WebPages.Title)
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.WebPages.Title)
                    </dd>
                    <dt>
                        @Html.DisplayNameFor(model => model.WebPages.Publish)
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.WebPages.Publish)
                    </dd>
                    <dt>
                        @Html.DisplayNameFor(model => model.WebPages.Content)
                    </dt>
                    <dd>
                        @Html.Raw(Model.WebPages.Content)
                    </dd>
                    <dt>
                        @Html.DisplayNameFor(model => model.WebPages.DateUtc)
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.WebPages.DateUtc)
                    </dd>
                    <dt>
                        @Html.DisplayNameFor(model => model.WebPages.PageType)
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.WebPages.PageType)
                    </dd>
                </dl>
            </div>
            <div>
                <a asp-page="./Edit" asp-route-id="@Model.WebPages.Id">Edit</a> |
                <a asp-page="./Index">Back to List</a>
            </div>
           

        </div>
